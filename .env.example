# ARM64 Environment Configuration
# Copy to .env and edit with your values

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Captcha Solver API ARM64

# Security - Change these!
API_KEYS=["your-secure-key-1", "your-secure-key-2"]

# CORS
BACKEND_CORS_ORIGINS=["*"]

# Model & Storage
MODEL_PATH=/app/best.pt
TEMP_RESULTS_DIR=/app/temp_results

# Rate Limiting
RATE_LIMIT=100

# ARM64 Optimization
# Adjust based on your CPU cores
OMP_NUM_THREADS=4
OPENBLAS_NUM_THREADS=4
